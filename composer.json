{
    "name": "jessegall/proxy-utils",
    "autoload": {
        "psr-4": {
            "JesseGall\\ProxyUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Jesse Gall"
        }
    ],
    "require": {
        "jessegall/proxy": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^6.1"
    },
    "scripts": {
        "test": "@php -dxdebug.mode=coverage ./vendor/bin/phpunit --testdox --coverage-html=report --colors=auto",
        "test-filter": "@test --filter="
    }
}
